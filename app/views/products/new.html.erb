<%= form_for(@product) do |f| %>
  <p>
    <%= f.label :name, "Product Name" %>
    <%= f.text_field :name %>
  </p>
  
<p>
    <%= f.label :price, "Price" %>
    <%= f.number_field :price %>
  </p>
  <p>
    <%= f.label :pictuer, "pictuer" %>
    <%= f.text_field :pictuer %>
  </p>
<p>
    <%= f.label :discreption, "discreption" %>
    <%= f.text_field :discreption %>
  </p>
  
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', products_path %>


